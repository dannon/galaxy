<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE visualization SYSTEM "../../visualization.dtd">
<visualization name="Video">
    <data_sources>
        <data_source>
            <model_class>HistoryDatasetAssociation</model_class>
            <test type="isinstance" test_attr="datatype" result_type="datatype">media.Mp4</test>
            <test type="isinstance" test_attr="datatype" result_type="datatype">media.Mkv</test>
            <test type="isinstance" test_attr="datatype" result_type="datatype">media.Flv</test>
            <to_param param_attr="id">dataset_id</to_param>
        </data_source>
    </data_sources>
    <params>
        <param type="dataset" var_name_in_template="hda" required="true">dataset_id</param>
    </params>
    <template>video.mako</template>
</visualization>
