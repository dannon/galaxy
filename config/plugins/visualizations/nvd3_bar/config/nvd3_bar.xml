<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE visualization SYSTEM "../../visualization.dtd">
<visualization name="Bar diagram (NVD3)" regular="True">
    <macros>
        <import>nvd3_shared.xml</import>
    </macros>
    <description>Renders a regular bar diagram using NVD3 hosted at http://www.nvd3.org.</description>
    <expand macro="data"/>
    <entry_point entry_point_type="chart" src="static/dist/nvd3.js" func="nvd3_bar"/>
    <specs>
        <expand macro="specs"/>
        <zoomable>true</zoomable>
    </specs>
    <groups>
        <expand macro="groups"/>
        <expand macro="groups_bar"/>
    </groups>
    <settings>
        <expand macro="settings"/>
    </settings>
</visualization>