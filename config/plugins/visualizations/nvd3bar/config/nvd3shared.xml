<macros>
    <xml name="default">
        <x_axis_label>
            <label>X-Axis label</label>
            <help>Provide a label for the axis.</help>
            <type>text</type>
            <value>X-axis</value>
            <placeholder>Axis label</placeholder>
        </x_axis_label>
        <x_axis_type>
            <name>x_axis_type</name>
            <type>conditional</type>
            <test_param>
                <name>type</name>
                <label>X-Axis value type</label>
                <type>select</type>
                <value>auto</value>
                <help>Select the value type of the axis.</help>
                <data>
                    <data>
                        <value>hide</value>
                        <label>-- Do not show values --</label>
                    </data>
                    <data>
                        <value>auto</value>
                        <label>Auto</label>
                    </data>
                    <data>
                        <value>d</value>
                        <label>Integer</label>
                    </data>
                    <data>
                        <value>e</value>
                        <label>Exponent</label>
                    </data>
                    <data>
                        <value>f</value>
                        <label>Float</label>
                    </data>
                    <data>
                        <value>p</value>
                        <label>Percent</label>
                    </data>
                    <data>
                        <value>s</value>
                        <label>SI-prefix</label>
                    </data>
                </data>
            </test_param>
            <cases>
                <cases>
                    <value>hide</value>
                </cases>
                <cases>
                    <value>auto</value>
                </cases>
                <cases>
                    <value>d</value>
                </cases>
                <cases>
                    <value>e</value>
                    <inputs>
                        <inputs>
                            <name>precision</name>
                            <label>Axis tick format</label>
                            <help>Select the tick format for the axis.</help>
                            <type>select</type>
                            <value>1</value>
                            <data>
                                <data>
                                    <label>0.00001</label>
                                    <value>5</value>
                                </data>
                                <data>
                                    <label>0.0001</label>
                                    <value>4</value>
                                </data>
                                <data>
                                    <label>0.001</label>
                                    <value>3</value>
                                </data>
                                <data>
                                    <label>0.01</label>
                                    <value>2</value>
                                </data>
                                <data>
                                    <label>0.1</label>
                                    <value>1</value>
                                </data>
                                <data>
                                    <label>1</label>
                                    <value>0</value>
                                </data>
                            </data>
                        </inputs>
                    </inputs>
                </cases>
